	<queryString><![CDATA[SELECT placeofdeliverycode, cname, vesselname, voyage, blno, blockcode, ID,
       ctype, csize, m2.placeofreturn AS placeofreturn,
       TO_CHAR (SYSDATE + 1, 'MM') AS smonth,
       TO_CHAR (SYSDATE + 1, 'DD') AS sday,
       TO_CHAR (SYSDATE + 8, 'MM') AS emonth,
       TO_CHAR (SYSDATE + 8, 'DD') AS eday, 
       TO_CHAR (SYSDATE, 'YYYY') AS cyear,
       TO_CHAR (SYSDATE, 'MM') AS cmonth, 
       TO_CHAR (SYSDATE, 'DD') AS cday,
       remark
  FROM (SELECT r1.placeofdeliverycode, r1.cname, r1.vesselname, r1.voyage,
               r1.blno, r1.ID, r1.ctype, r1.csize, r1.remark, r1.svcname,
               r1.portofloadingcode,
               DECODE (r1.hot_stow,
                       'Y', NVL (b1.block_code, r1.blockcode),
                       r1.blockcode
                      ) AS blockcode
          FROM (SELECT b.placeofdeliverycode, d.cname,
                       TRIM (v.NAME) AS vesselname,
                       DECODE (mv.rvslvoyid,
                               NULL, SUBSTR (b.vslvoyid, 5, 3),
                               SUBSTR (mv.rvslvoyid, 5)
                              ) AS voyage,
                       'APLU0' || b.blnumber AS blno, c.ID,
                       SUBSTR (c.ctncode, 1, 3) AS ctype,
                       SUBSTR (c.ctncode, 4, 4) AS csize, nvl2(s.sremark,s.remark||'#'||s.sremark||'#',s.remark) as remark, v.svcname,
                       b.portofloadingcode, b.hot_stow, b.blockcode
                  FROM ctn_dist c,
                       booking b,
                       bookingstatus s,
                       vessel v,
                       depot d,
                       (SELECT v.vslcd || ' '
                               || v.external_voyid AS rvslvoyid,
                               v.vslcd || ' ' || v.internal_voyid AS vslvoyid
                          FROM vslcd_ivoy_evoy v
                         WHERE v.status = 'Y') mv
                 WHERE c.ID = $P{ctnid}
                   AND c.blnumber = b.blnumber
                   AND c.blnumber = s.blnumber
                   AND SUBSTR (b.vslvoyid, 1, 3) = v.ID(+)
                   AND c.depotcode = d.depotcode(+)
                   AND b.vslvoyid = mv.vslvoyid(+)) r1,
               (SELECT svcname, portofloadingcode, block_code
                  FROM block_code_update
                 WHERE block_code_update.status = 'Y') b1
         WHERE r1.svcname = b1.svcname(+) AND r1.portofloadingcode = b1.portofloadingcode(+)) r2,
       (SELECT *
          FROM mapstp
         WHERE mapstp.status = 'Y') m2
 WHERE r2.svcname = m2.svcname(+) AND r2.portofloadingcode = m2.portofloadingcode(+)]]></queryString>

