	<queryString><![CDATA[select b.placeofdeliverycode,
       d.cname,
       b.vesselname,
       b.voyage,
       'APLU0'||b.blnumber as blno,
       decode(b.hot_stow,'Y',decode(v.svcname,'PCX','USSQ2','PS3','ZZOPT',b.blockcode),b.blockcode) as blockcode,
       c.id,
       substr(c.ctncode,1,3) as ctype, 
       substr(c.ctncode,4,4) as csize,
       c.ctnstatus,
       m.placeofreturn as placeofreturn,
       to_char(sysdate+1,'MM') as smonth,
       to_char(sysdate+1,'DD') as sday,
       to_char(sysdate+8,'MM') as emonth,
       to_char(sysdate+8,'DD') as eday,
       to_char(sysdate,'YYYY') as cyear,
       to_char(sysdate,'MM') as cmonth,
       to_char(sysdate,'DD') as cday,
       s.remark
from booking b, 
     bookingstatus s,
     container c,
     vessel v,
     mapstp m,
     (SELECT ac.blnumber,ac.ctncode,ac.depotcode,ac.status,bd.cname
           FROM CTN_DIST ac, depot bd
        Where ac.depotcode=bd.depotcode
          AND ac.blnumber=$P{blnumber}
          and bd.depotcode in (SELECT p.depot FROM users u, user_profiles p WHERE u.username like $P{dusername} and u.userid=p.userid)
          GROUP BY ac.blnumber,ac.ctncode,ac.depotcode,ac.status,bd.cname) d
where b.blnumber = $P{blnumber}
  and b.blnumber = s.blnumber
  and b.blnumber = c.blnumber (+)
  and substr(b.vslvoyid,1,3) = v.id (+)
  and v.svcname = m.svcname (+)
  and c.ctncode = d.ctncode]]></queryString>

